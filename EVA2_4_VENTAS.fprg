    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Pieza" type="Integer" array="False" size=""/>
            <declare name="calcSaldo" type="Integer" array="False" size=""/>
            <output expression="&quot;Numero de piezas que desea comprar&quot;" newline="True"/>
            <input variable="Pieza"/>
            <if expression="Pieza &gt; 1000">
                <then>
                    <assign variable="calcSaldo" expression="Pieza * 500"/>
                </then>
                <else>
                    <assign variable="calcSaldo" expression="Pieza * 800"/>
                </else>
            </if>
            <output expression="&quot;El total es: &quot;" newline="True"/>
            <output expression="calcSaldo" newline="True"/>
        </body>
    </function>
